#         [OPTIONS]
# ---------------------------

# -- How to use -- 
# [?] ./gpk -h | --help = Show Help Menu
# [?] ./gpk + parameter = Desired Option

while [ $# -gt 0 ]; do
  case "$1" in
      -h|--help)
      help
      exit
      ;;
      -m)
      machineinfo
      exit
      ;;
      -ds)
      deepsearch
      ;;
      -n)
      network
      ;;
      -c2)
      chacal-c2
      ;;
      -p)
      root_check
      persistence
      ;;
      *)
      echo -e "
 [$Rd""Error!""$We] Invalid option: "$Yw""$1""$We"
 
  Try $Gn-h$We or $Gn--help$We for more informations"
      exit
      ;;
  esac
  shift
done

