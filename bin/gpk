PS3="
$(echo -e "$tip")

[GPK]> "
options=(
"Exit"
"Clear"
"MachineInfo"
"DeepSearch"
"Network"
"ChacalC2"
"Persistence"
)

 select var in "${options[@]}"; do
    case $var in

"Exit")
echo -e "$We"
exit
;;
"Clear")
clear
;;
"MachineInfo")
clear
machineinfo
;;
"DeepSearch")
clear
deepsearch
;;
"Network")
network
;;
"ChacalC2")
chacal-c2
;;
"Persistence")
root_check
persistence
;;
*)
  echo
  echo -e "$Rd"  Invalid Option!"$We"
    esac
    echo
done
